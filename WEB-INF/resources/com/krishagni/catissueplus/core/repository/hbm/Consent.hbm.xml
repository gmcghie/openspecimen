<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class name="com.krishagni.catissueplus.core.biospecimen.domain.CpConsent" table="OS_CP_CONSENTS">
    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">OS_CP_CONSENT_SEQ</param>
      </generator>
    </id>
    
    <property name="code" column="CODE"/>

    <property name="statement" column="STATEMENT"/>

    <property name="activityStatus" column="ACTIVITY_STATUS"/>
    
    <filter name="activeEntity"/>
    
    <query name="getCpConsentByCode">
      from
        com.krishagni.catissueplus.core.biospecimen.domain.CpConsent consent
      where
        consent.code = :code
    </query>
  </class>
</hibernate-mapping>
